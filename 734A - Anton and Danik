#include <iostream>

using namespace std;

int main(){
    int n,i,a=0,d=0;
    cin >> n;
    string s;
    
    for(i=0;i<n;i++){
        cin >> s;
    }
    for(i=0;i<n;i++){
        if (s[i] == 'D')
            d++;
        else
            a++;
    }
    
    if (a>d)
        cout << "Anton" << endl;
    else if (a<d)
        cout << "Danik" << endl;
    else if (a==d)
        cout << "Friendship" << endl;
    
    
            
    return 0;
}
